class PrayerMonth {
  final String userId;
  final String year;
  final String month;
  final int totalprayers;
  final int offeredprayers;

  PrayerMonth({required this.userId, required this.year, required this.month, required this.totalprayers, required this.offeredprayers,});

  Map<String, dynamic> toMap() {
    return {
      "userID": senderId,
      "year": senderEmail,
      "month": recieverID,
      "totalprayers": message,
      "offeredprayers": offeredprayers,
    };
  }
}
